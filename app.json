{
  "expo": {
    "scheme": "myapp",
    "name": "Game Fix It",
    "slug": "gamefixit",
    "version": "1.0.0",
    "web": {
      "favicon": "./logo.jpeg",
      "bundler": "metro"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "orientation": "portrait",
    "icon": "./logo.jpeg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./logo.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./logo.jpeg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nikhilchaudhary121.gamefixit"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0b5b0604-25e2-4573-a6b1-84552fab3667"
      }
    }
  }
}
